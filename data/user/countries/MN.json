        "output_format": "{% if has_recipient %}{{ recipient_text }}\n{% endif %}{% if has_street %}{{ street_text }}\n{% endif %}{% if has_postcode %}{{ postcode_text }}\n{% endif %}{% if has_settlement %}{{ settlement_text }}\n{% endif %}{% if has_level1area %}{{ level1area_name }}\n{% endif %}{% if has_country %}{{ country_name }}\n{% endif %}",
        "level1area_names": {
            "en": "Province"
        },
        "level2area_names": null,
        "settlement_names": {
            "en": "Settlement"
        },
        "postcode_names": {
            "en": "Post Code"
